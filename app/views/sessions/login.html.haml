-#Navigation Bar
%nav.navbar.navbar-dark.bg-dark
    %a{:style=>'color: white', :href=>'#'} Logo
-#Container
%div.container{:style=>'margin: 0px; margin-top: 10px'}
    %h2 Musicman login 

    = form_tag callback_path(), :method => :get do

        = label :user, :name, 'User Name'
        = text_field :user, :name

        = label :user, :password, 'Password'
        = password_field :user, :password

        =submit_tag 'Login'
